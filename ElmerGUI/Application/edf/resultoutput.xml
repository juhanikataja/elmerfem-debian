<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE edf>
<edf version="1.0" >
  <PDE Name="Result Output" >
    <Name> Result Output </Name>
    
    <Equation>
    </Equation>

    <Solver>
      <Parameter Widget="Edit" >
        <Name> Procedure </Name>
        <DefaultValue> "ResultOutputSolve" "ResultOutputSolver" </DefaultValue>
